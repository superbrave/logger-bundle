{
    "name": "superbrave/logger-bundle",
    "description": "A Symfony bundle to log messages to logging providers. Such as Sentry.",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "Tim van den Oever",
            "email": "tvdo@superbrave.nl"
        }
    ],
    "require": {
        "php": "^7.2",
        "sentry/sentry-symfony": "^3.0",
        "symfony/config": "^3.4||^4.0||^5.0",
        "symfony/dependency-injection": "^3.4||^4.0||^5.0"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^3.4||^4.0||^5.0",
        "friendsofphp/php-cs-fixer": "^2.16"
    },
    "license": "MIT",
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Superbrave\\LoggerBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Superbrave\\LoggerBundle\\Tests\\": "tests/"
        }
    }
}
